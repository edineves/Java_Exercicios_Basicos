//SOMA DE NUMEROS
package listaDeExercicios;
import java.util.*;

public class ex1 {

	public static void main(String[] args) {		
		Scanner sc = new Scanner( System.in);
		int n1,n2,sum;
		
		System.out.println("Digite um numero o primeiro numero: ");
		n1 = sc.nextInt();
		System.out.println("Digite o segundo número: ");
		n2 = sc.nextInt();
		
		sum = (n1+n2);		
		System.out.print("O resultado é: " +sum);			
		
		
	}

}




